#: D:\code\linq2hash\src\hc\builtin.c:29 D:\code\linq2hash\src\l2h\hashes.c:175
#, c-format
msgid "Unknown hash: %s"
msgstr "Неизвестный хэш: %s"

#: D:\code\linq2hash\src\hc\builtin.c:90
#, c-format
msgid "\n"
" --sfv option doesn't support %s algorithm. Only crc32 or crc32c supported"
msgstr "\n"
"опция --sfv не подерживается в алгоритме %s. Применимо только для crc32 или crc32c"

#: D:\code\linq2hash\src\hc\configuration.c:30
msgid "set the limit in bytes of the part of the file to calculate hash for. The whole file by default will be applied"
msgstr "количество байт файла для вычисления хэша. По умолчанию будет использоваться весь файл"

#: D:\code\linq2hash\src\hc\configuration.c:34
msgid "set start position within file to calculate hash from. Zero by default"
msgstr "стартовая позиция в файле для вычисления хэша. По умолчанию начало файла"

#: D:\code\linq2hash\src\hc\configuration.c:38
msgid "print this help and exit"
msgstr "показать помощь и выйти"

#: D:\code\linq2hash\src\hc\configuration.c:42
msgid "show calculation time (false by default)"
msgstr "показывать время вычислений (по умолчанию нет)"

#: D:\code\linq2hash\src\hc\configuration.c:46
msgid "output hash using low case (false by default)"
msgstr "выход хэша с использованием нижнего регистра (по умолчанию  в верхнем)"

#: D:\code\linq2hash\src\hc\configuration.c:50
msgid "output hash in file checksum format"
msgstr "вывод формате файла контрольной суммы"

#: D:\code\linq2hash\src\hc\configuration.c:53
msgid "output hash in the SFV (Simple File Verification)  format (false by default). Only for CRC32 or CRC32C."
msgstr "выводить в виде SFV (Simple File Verification). Только для CRC32 или CRC32C. По умолчанию нет."

#: D:\code\linq2hash\src\hc\configuration.c:56
msgid "Disable hash crack time probing (how much time it may take)"
msgstr "Отключить оценку времени восстановления строки (сколько примерно это может занять)"

#: D:\code\linq2hash\src\hc\configuration.c:59
msgid "Disable error output while search files. False by default."
msgstr "Отключить вывод ошибок во время поиска файлов. По умолчанию выводить."

#: D:\code\linq2hash\src\hc\configuration.c:63
msgid "the number of threads to crack hash. The half of system processors by default. The value must be between 1 and processor count."
msgstr "количество потоков, чтобы взломать хэш. Половина количества процессоров системы по умолчанию. Значение должно быть между 1 и числом равным количеству процессоров."

#: D:\code\linq2hash\src\hc\configuration.c:67
msgid "save files' hashes into the file specified instead of console."
msgstr "сохранение хэшей файлов в указанный файл, вместо консоли."

#: D:\code\linq2hash\src\hc\configuration.c:69
msgid "hash algorithm. See all possible values below"
msgstr "хэш алгоритм. См. возможные значения ниже"

#: D:\code\linq2hash\src\hc\configuration.c:82
msgid "output hash as Base64"
msgstr "выводить хэш в base64"

#: D:\code\linq2hash\src\hc\configuration.c:126
msgid "must be string"
msgstr "должно быть string"

#: D:\code\linq2hash\src\hc\configuration.c:127
msgid "must be hash"
msgstr "должно быть hash"

#: D:\code\linq2hash\src\hc\configuration.c:128
msgid "must be file"
msgstr "должно быть file"

#: D:\code\linq2hash\src\hc\configuration.c:129
msgid "must be dir"
msgstr "должно быть dir"

#: D:\code\linq2hash\src\hc\configuration.c:131
msgid "string to calculate hash sum for"
msgstr "строка для которой вычисляется хэш"

#: D:\code\linq2hash\src\hc\configuration.c:132
msgid "hash to restore initial string by"
msgstr "хэш по которому будет восстанавливаться строка"

#: D:\code\linq2hash\src\hc\configuration.c:133
msgid "full path to file to calculate hash sum of"
msgstr "полный путь к файлу для вычисления хэш-суммы"

#: D:\code\linq2hash\src\hc\configuration.c:134
msgid "full path to dir to calculate all content's hashes"
msgstr "полный путь к каталогу для вычисления хэша всех его файлов"

#: D:\code\linq2hash\src\hc\configuration.c:136
msgid "exclude files that match the pattern specified. It's possible to use several patterns separated by ;"
msgstr "исключать файлы удовлетворяющие заданному шаблону. Возможно указывать несколько шаблонов, разделяя их ;"

#: D:\code\linq2hash\src\hc\configuration.c:137
msgid "include only files that match the pattern specified. It's possible to use several patterns separated by ;"
msgstr "использовать только те файлы, что удовлетворяют заданному шаблону. Возможно указывать несколько шаблонов, разделяя их ;"

#: D:\code\linq2hash\src\hc\configuration.c:139
msgid "hash to validate file"
msgstr "хэш для проверки файла"

#: D:\code\linq2hash\src\hc\configuration.c:140
msgid "hash to validate files in directory"
msgstr "хэш для проверки файлов в каталоге"

#: D:\code\linq2hash\src\hc\configuration.c:141
msgid "interpret hash as Base64"
msgstr "хэш задан в форме base64"

#: D:\code\linq2hash\src\hc\configuration.c:148
msgid "initial string's dictionary. All digits, upper and lower case latin symbols by default. Specify value ASCII here to use all ASCII charset as dictionary"
msgstr "словарь строки. Все цифры, латинские буквы в верхнем и нижнем регистре по умолчанию. Укажите значение ASCII чтобы использовать весь набор ASCII как словарь"

#: D:\code\linq2hash\src\hc\configuration.c:149
msgid "set minimum length of the string to restore. 1 by default"
msgstr "минимальная длина строки для восстановления. По умолчанию 1"

#: D:\code\linq2hash\src\hc\configuration.c:153
msgid "set maximum length of the string to restore. 10 by default"
msgstr "максимальная длина строки для восстановления. По умолчанию 10"

#: D:\code\linq2hash\src\hc\configuration.c:158
msgid "hash to search a file that matches it"
msgstr "хэш для поиска файла который ему соответствует"

#: D:\code\linq2hash\src\hc\configuration.c:160
msgid "scan directory recursively"
msgstr "включая вложенные каталоги"

#: D:\code\linq2hash\src\hc\configuration.c:161
msgid "test performance by cracking 12345 string hash"
msgstr "тест производительности путем восстановления  хэша строки 12345"

#: D:\code\linq2hash\src\hc\configuration.c:211
#, c-format
msgid "Invalid command one of: %s, %s, %s or %s expected"
msgstr "Неверная команда ожидается одна из: %s, %s, %s or %s"

#: D:\code\linq2hash\src\hc\configuration.c:348
#: D:\code\linq2hash\src\l2h\processor.c:473
#, c-format
msgid "Threads number must be between 1 and %u but it was set to %lu. Reset to default %u"
msgstr "Количество потоков должно быть между 1 и %u но было задано %lu. Используется значение %u"

#: D:\code\linq2hash\src\hc\configuration.c:365
#, c-format
msgid "Invalid %s option must be positive but was %lli"
msgstr "Ошибка параметр %s должен быть положительным но был %lli"

#: D:\code\linq2hash\src\hc\dir.c:81 D:\code\linq2hash\src\hc\file.c:55
#, c-format
msgid "\n"
"Error opening file: %s Error message: "
msgstr "\n"
"Ошибка открытия файла: %s Сообщение об ошибке: "

#: D:\code\linq2hash\src\l2h\hashes.c:96
msgid "  Supported hash algorithms:"
msgstr " Поддерживаемые хэш алгоритмы:"

#: D:\code\linq2hash\src\srclib\bf.c:109
msgid "Empty string"
msgstr "Пустая строка"

#: D:\code\linq2hash\src\srclib\bf.c:149
#, c-format
msgid "May take approximatelly: %s (%s attempts)"
msgstr "Может занять примерно: %s (%s попыток)"

#: D:\code\linq2hash\src\srclib\bf.c:163
#, c-format
msgid "Initial string is: %s"
msgstr "Исходная строка: %s"

#: D:\code\linq2hash\src\srclib\bf.c:165
msgid "Nothing found"
msgstr "Ничего не найдено"

#: D:\code\linq2hash\src\srclib\bf.c:177
#, c-format
msgid "Attempts: %s Time "
msgstr "Попыток: %s Время "

#: D:\code\linq2hash\src\srclib\bf.c:179
#, c-format
msgid " Speed: %s attempts/second"
msgstr " Скорость: %s попыток в секунду"

#: D:\code\linq2hash\src\srclib\bf.c:201
#, c-format
msgid "Max string length is too big: %lu"
msgstr "Максимальная длина строки слишком велика: %lu"

#: D:\code\linq2hash\src\srclib\filehash.c:40
msgid "File is valid"
msgstr "Файл верный"

#: D:\code\linq2hash\src\srclib\filehash.c:41
msgid "File is invalid"
msgstr "Файл неверный"

#: D:\code\linq2hash\src\srclib\filehash.c:115
msgid "Offset is greater then file size"
msgstr "Смещение больше чем размер файла"

